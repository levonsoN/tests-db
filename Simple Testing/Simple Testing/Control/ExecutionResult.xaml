<UserControl x:Class="SimpleTesting.Control.ExecutionResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SimpleTesting.Control"
             mc:Ignorable="d" Height="75" Width="315">
    <Grid Background="White" Height="{Binding ElementName=control, Path=ActualHeight}" Width="{Binding ElementName=control, Path=ActualWidth}">
        <Label Content="Execution Time: " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Points Gained: " HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
